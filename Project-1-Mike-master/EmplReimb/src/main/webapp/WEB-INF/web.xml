<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
	<servlet>
		<servlet-name>empl-servlet</servlet-name>
		<servlet-class>com.servlets.EmployeeServlet</servlet-class>
	</servlet>
	
	 <servlet>
			<servlet-name>newReimb-servlet</servlet-name>
			<servlet-class>com.servlets.ReimbServlet</servlet-class>
	  </servlet>
  
	  <servlet>
	  		<servlet-name>SessionServlet</servlet-name>
	  		<servlet-class>com.servlets.SessionServlet</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>DriverClass</servlet-name>
	  	<servlet-class>com.servlets.DriverClass</servlet-class>
	  	<load-on-startup>0</load-on-startup>
	  </servlet>
	  <servlet>
	  	<servlet-name>DisplayServlet</servlet-name>
	  	<servlet-class>com.servlets.AllReimbServlet</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>AdminServlet</servlet-name>
	  	<servlet-class>com.servlets.AdminServlet</servlet-class>
	  </servlet>
	 
	  <servlet-mapping>
		<servlet-name>empl-servlet</servlet-name>
		<url-pattern>/users</url-pattern>
		</servlet-mapping>
		
	  <servlet-mapping>
			<servlet-name>newReimb-servlet</servlet-name>
			<url-pattern>/create</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	  		<servlet-name>SessionServlet</servlet-name>
	  		<url-pattern>/SessionServlet</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	  	<servlet-name>DriverClass</servlet-name>
	  	<url-pattern>/DriverClass</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	  	<servlet-name>DisplayServlet</servlet-name>
	  	<url-pattern>/display/*</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	  	<servlet-name>AdminServlet</servlet-name>
	  	<url-pattern>/admin</url-pattern>
	  </servlet-mapping>
</web-app>